@model IEnumerable<Voting.Models.Candidate>

@{
    ViewData["Title"] = "Default";
}

<h1>Candidates</h1>
<table class="table">
    <thead>
        <tr>
           
            <th>
                @Html.DisplayNameFor(model => model.CandidateName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Year)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Photo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.CandidateName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Year)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Photo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CanId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CanId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CanId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
