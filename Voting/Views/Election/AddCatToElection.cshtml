@model IEnumerable<Voting.Models.Category>

@{
    ViewData["Title"] = "AddCatToElection";
}

<h1>Add Categories To Election( @ViewBag.Description)</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CategoryName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>
            <td>
                <a  asp-action="AddCatToElectionPost" asp-route-catId="@item.CatId" asp-route-guid="@ViewBag.Guid">Add</a>
            </td>
        </tr>
}
    </tbody>
</table>
