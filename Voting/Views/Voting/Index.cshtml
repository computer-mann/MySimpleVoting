@model IEnumerable<Voting.Models.Category>

@{
    ViewData["Title"] = "Index";
}

<h1>Categories</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CategoryName)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CategoryName)
            </td>

            <td>
                <a asp-action="CatIndex" asp-route-catId="@item.CatId">Go to Category</a> 
            </td>
        </tr>
}
    </tbody>
</table>
@if (Model.Count() == 0)
{
<h3><a class="btn-link" asp-action="Done">Click Here When Done</a></h3>

}