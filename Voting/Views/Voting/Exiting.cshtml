@model List<Voting.Models.Votes>

@{
    ViewData["Title"] = "Exiting";
}

<h1>You voted for</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Category Name
            </th>
            <th>
                Candidate 
            </th>
            
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Category.CategoryName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Candidate.CandidateName)
            </td>
        </tr>
}
        
    </tbody>
</table>
<h3><a class="btn-link" asp-area="" asp-controller="Home" asp-action="Index">Click Here When Done</a></h3>
